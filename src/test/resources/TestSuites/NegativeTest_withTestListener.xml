<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="NegativeTestSuite" verbose="1" >
    <listeners>
        <listener class-name="com.herokuapp.base.TestListener"></listener>
    </listeners>
    <test name="NegativeTest">
        <groups>
            <run>
                <include name="smokeTests"/>
            </run>
        </groups>

        <classes>
            <class name="com.herokuapp.theinternetLoginTest.NegativeLoginTest"/>
        </classes>
    </test>
</suite>
