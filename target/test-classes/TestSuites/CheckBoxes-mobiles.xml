<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="CheckBoxes with mobiles" verbose="1" >
    <test name="CheckBoxes Android" >
        <parameter name="deviceName" value="Pixel 2 XL"></parameter>
        <classes>
            <class name="com.herokuapp.theinternetLoginTest.Test_Pages_CheckBox" />
        </classes>
    </test>

    <test name="CheckBoxes Android2" >
        <parameter name="deviceName" value="Pixel 5"></parameter>
        <classes>
            <class name="com.herokuapp.theinternetLoginTest.Test_Pages_CheckBox" />
        </classes>
    </test>

    <test name="CheckBoxes iOS" >
        <parameter name="deviceName" value="iPhone SE"></parameter>
        <classes>
            <class name="com.herokuapp.theinternetLoginTest.Test_Pages_CheckBox" />
        </classes>
    </test>
</suite>